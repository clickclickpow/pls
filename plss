			TextButton.TextScaled = true
			TextButton.TextSize = 14
			TextButton.TextWrapped = true
			TextButton.Name = c.Name
			TextButton.Text = c.Name
			TextButton.MouseButton1Click:connect(function()
				local fusestats2 = script.Parent.Parent["Fuse Stats Show"]
				fusestats2.Strength.Text = "Strength: "
				fusestats2.Speed.Text = "Speed: "
				fusestats2.Ki.Text = "Ki: "
				fusestats2.Rebirth.Text = "Rebirth: "
				fusestats2.PowerLevel.Text = "Fusion Power Level: "
				
