				uigridlayout.CellSize = UDim2.new(0.95, 0, 0.04, 0)
				uigridlayout.Parent = script.Parent.Parent.Backpack
				
				for _,z in pairs(game.Players[c.Name].Backpack:GetChildren()) do
					local TextButton2 = Instance.new("TextButton")
