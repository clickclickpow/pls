					TextButton1.TextSize = 14
					TextButton1.TextWrapped = true
					TextButton1.Name = j.Name
					TextButton1.Text = j.Name
					if TextButton1.Text == script.Parent.Parent[" PlrName"].Text then
						TextButton1:remove()
					end
					TextButton1.MouseButton1Click:connect(function()
						FuseStats.Strength.Text = "Strength: "..datas[script.Parent.Parent[" PlrName"].Text].Strength.Value + datas[j.Name].Strength.Value
						FuseStats.Speed.Text = "Speed: "..datas[script.Parent.Parent[" PlrName"].Text].Speed.Value + datas[j.Name].Speed.Value
						FuseStats.Ki.Text = "Ki: "..datas[script.Parent.Parent[" PlrName"].Text].Ki.Value + datas[j.Name].Ki.Value
						FuseStats.Rebirth.Text = "Rebirth: "..datas[script.Parent.Parent[" PlrName"].Text].Rebirth.Value + datas[j.Name].Rebirth.Value
						FuseStats.PowerLevel.Text = "Fusion Power Level: "..tostring((datas[script.Parent.Parent[" PlrName"].Text].Strength.Value + datas[j.Name].Strength.Value)*(datas[script.Parent.Parent[" PlrName"].Text].Rebirth.Value/10 + datas[j.Name].Rebirth.Value/10)+(datas[script.Parent.Parent[" PlrName"].Text].Strength.Value + datas[j.Name].Strength.Value))
					end)
				end
			end)
		end
		
	end
	coroutine.resume(coroutine.create(SCRIPT_UUOU84_FAKESCRIPT))

end)
Train:Button("Inf Ki On", function() -- This is A Button
    local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.RightHand.RightGripAttachment,
    [2] = game:GetService("Players").LocalPlayer.Character.LeftHand.LeftGripAttachment,
    [3] = -99999999
}

game:GetService("ReplicatedStorage").Events.volley:FireServer(unpack(args))

end)
Train:Button("Inf Ki Off", function() -- This is A Button
    local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.RightHand.RightGripAttachment,
    [2] = game:GetService("Players").LocalPlayer.Character.LeftHand.LeftGripAttachment,
    [3] = 99999999
}

game:GetService("ReplicatedStorage").Events.volley:FireServer(unpack(args))

end)


Autofarm:Label("Hello") -- This is just for information or Text ;)

Autofarm:line() -- This Creates a line aka some space for a new section or something like that :P
