Buttons:Button("Scouter", function() -- This is A Button
    
	local ScouterBest = Instance.new("ScreenGui")
	local Main = Instance.new("Frame")
	local List = Instance.new("Frame")
	local PlrList = Instance.new("ScrollingFrame")
	local UIGridLayout = Instance.new("UIGridLayout")
	local Reload = Instance.new("TextButton")
	local PlrName = Instance.new("TextLabel")
	local BaseStats = Instance.new("TextLabel")
	local Strength = Instance.new("TextLabel")
	local Speed = Instance.new("TextLabel")
	local Ki = Instance.new("TextLabel")
	local Rebirth = Instance.new("TextLabel")
	local PLlvl = Instance.new("TextLabel")
	local FuseStatsShow = Instance.new("TextLabel")
	local Strength_2 = Instance.new("TextLabel")
	local Speed_2 = Instance.new("TextLabel")
	local Ki_2 = Instance.new("TextLabel")
	local Rebirth_2 = Instance.new("TextLabel")
	local PowerLevel = Instance.new("TextLabel")
	local Backpackname = Instance.new("TextLabel")
	local FusePlrList = Instance.new("ScrollingFrame")
	local UIGridLayout_2 = Instance.new("UIGridLayout")
	local WhoFusedWith = Instance.new("TextLabel")
	local Backpack = Instance.new("ScrollingFrame")
	local UIGridLayout_3 = Instance.new("UIGridLayout")
	local SSS = Instance.new("TextLabel")
	local OpenClose = Instance.new("TextButton")
	warn("\109\97\100\101\32\98\121\32\122\122\111\116\97\99\32\110\111\116\32\101\118\101\110\32\115\116\111\108\101\110")
	
	--Properties:
	ScouterBest.Name = "ScouterBest"
	ScouterBest.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	ScouterBest.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	
	Main.Name = "Main"
	Main.Parent = ScouterBest
	Main.BackgroundColor3 = Color3.new(1, 1, 0)
	Main.BackgroundTransparency = 0.5
	Main.BorderColor3 = Color3.new(1, 1, 0)
	Main.BorderSizePixel = 0
	Main.Position = UDim2.new(0.112000003, 0, 0.136999995, 0)
	Main.Size = UDim2.new(0.841535449, 0, 0.77177912, 0)
	Main.Visible = false
	
	List.Name = "List"
	List.Parent = Main
	List.BackgroundColor3 = Color3.new(1, 1, 0)
	List.BackgroundTransparency = 0.5
	List.BorderColor3 = Color3.new(1, 1, 0)
	List.BorderSizePixel = 0
	List.Position = UDim2.new(-0.13333334, 0, 0.0985691547, 0)
	List.Size = UDim2.new(0.13333334, 0, 0.790143073, 0)
	
	PlrList.Name = "PlrList"
	PlrList.Parent = List
	PlrList.BackgroundColor3 = Color3.new(1, 1, 1)
	PlrList.BackgroundTransparency = 1
	PlrList.Position = UDim2.new(0, 0, 0.0744466782, 0)
	PlrList.Size = UDim2.new(1, 0, 0.925553322, 0)
	
	UIGridLayout.Parent = PlrList
	UIGridLayout.CellPadding = UDim2.new(0, 0, 0, 5)
	UIGridLayout.CellSize = UDim2.new(0.920000017, 0, 0.0399999991, 0)
	
	Reload.Name = "Reload"
	Reload.Parent = List
	Reload.BackgroundColor3 = Color3.new(1, 1, 0)
	Reload.BackgroundTransparency = 0.69999998807907
	Reload.BorderSizePixel = 0
	Reload.Position = UDim2.new(-0.000883483852, 0, -2.2412305e-05, 0)
	Reload.Size = UDim2.new(1.00088346, 0, 0.0754895061, 0)
	Reload.Font = Enum.Font.SourceSans
	Reload.Text = "Reload"
	Reload.TextColor3 = Color3.new(0, 0, 0)
	Reload.TextScaled = true
	Reload.TextSize = 14
	Reload.TextWrapped = true
	
	PlrName.Name = " PlrName"
	PlrName.Parent = Main
