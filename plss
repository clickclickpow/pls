	function SCRIPT_DVAZ71_FAKESCRIPT() -- Reload.LocalScript 
		local script = Instance.new('LocalScript')
		script.Parent = Reload
		script.Parent.MouseButton1Click:Connect(function()
			for _,x in pairs(script.Parent.Parent.PlrList:GetChildren()) do
				x:remove()
				wait()
				local uigridlayout = Instance.new("UIGridLayout")
				uigridlayout.CellSize = UDim2.new(0.92, 0, 0.04, 0)
				uigridlayout.CellPadding = UDim2.new(0, 0, 0, 5)
				uigridlayout.Parent = script.Parent.Parent.PlrList
				
				local stats34 = script.Parent.Parent.Parent["Base Stats"]
				stats34.Strength.Text = "Strength: "
				stats34.Speed.Text  = "Speed: "
				stats34.Ki.Text  = "Ki: "
				stats34.Rebirth.Text  = "Rebirth: "
				stats34.PLlvl.Text  = "Base Power Level: "
				
				local Fusestats34 = script.Parent.Parent.Parent["Base Stats"]
				Fusestats34.Strength.Text = "Strength: "
				Fusestats34.Speed.Text  = "Speed: "
				Fusestats34.Ki.Text  = "Ki: "
				Fusestats34.Rebirth.Text  = "Rebirth: "
				Fusestats34.PLlvl.Text  = "Fusion Power Level: "		
			end	
		end)
		
		script.Parent.MouseButton1Click:Connect(function()
			wait()
			for _,n in pairs(game.Players:GetChildren()) do
				local datas = game.ReplicatedStorage.Datas[n.Name]
				local textler = script.Parent.Parent.Parent["Base Stats"]	
				local TextButton = Instance.new("TextButton")
				TextButton.Parent = script.Parent.Parent.PlrList
				TextButton.BackgroundColor3 = Color3.new(0.792157, 0.792157, 0)
				TextButton.Size = UDim2.new(0, 200, 0, 50)
				TextButton.Font = Enum.Font.SourceSans
				TextButton.TextColor3 = Color3.new(1, 1, 0)
				TextButton.TextScaled = true
				TextButton.TextSize = 14
				TextButton.TextWrapped = true
				TextButton.Name = n.Name
				TextButton.Text = n.Name
				TextButton.MouseButton1Click:connect(function()
					textler.Strength.Text = "Strength: "..datas.Strength.Value
					local fusestats2 = script.Parent.Parent.Parent["Fuse Stats Show"]
					fusestats2.Strength.Text = "Strength: "
					fusestats2.Speed.Text = "Speed: "
					fusestats2.Ki.Text = "Ki: "
					fusestats2.Rebirth.Text = "Rebirth: "
					fusestats2.PowerLevel.Text = "Fusion Power Level: "
					
					local uigridlayout = Instance.new("UIGridLayout")
					uigridlayout.CellSize = UDim2.new(0.95, 0, 0.04, 0)
					uigridlayout.Parent = script.Parent.Parent.Parent.Backpack
					
					for _,z in pairs(game.Players[n.Name].Backpack:GetChildren()) do
						local TextButton2 = Instance.new("TextButton")
						TextButton2.Parent = script.Parent.Parent.Parent.Backpack
						TextButton2.BackgroundColor3 = Color3.new(0.921569, 0.921569, 0)
						TextButton2.Size = UDim2.new(0, 200, 0, 50)
						TextButton2.Font = Enum.Font.SourceSans
						TextButton2.Text = z.Name
						TextButton2.TextColor3 = Color3.new(0, 0, 0)
						TextButton2.TextScaled = true
						TextButton2.TextSize = 14
						TextButton2.TextWrapped = true
						TextButton2.Text = z.Name
						if TextButton2.Text == "Combat" then
							TextButton2:remove()
						end
					end
					wait()
					for _,t in pairs(script.Parent.Parent.Parent.Backpack:GetChildren()) do
						t:remove()
					end
					wait()
					local uigridlayout = Instance.new("UIGridLayout")
					uigridlayout.CellSize = UDim2.new(0.95, 0, 0.04, 0)
					uigridlayout.Parent = script.Parent.Parent.Parent.Backpack
					
					for _,o in pairs(game.Players[n.Name].Backpack:GetChildren()) do
						local TextButton3 = Instance.new("TextButton")
						TextButton3.Parent = script.Parent.Parent.Parent.Backpack
						TextButton3.BackgroundColor3 = Color3.new(0.921569, 0.921569, 0)
						TextButton3.Size = UDim2.new(0, 200, 0, 50)
						TextButton3.Font = Enum.Font.SourceSans
						TextButton3.Text = o.Name
						TextButton3.TextColor3 = Color3.new(0, 0, 0)
						TextButton3.TextScaled = true
						TextButton3.TextSize = 14
						TextButton3.TextWrapped = true
						TextButton3.Text = o.Name
						if TextButton3.Text == "Combat" then
							TextButton3:remove()
						end
					end	
					textler.Strength.Text = "Strength: "..datas.Strength.Value
					textler.Speed.Text = "Speed: "..datas.Speed.Value
					textler.Ki.Text = "Ki: "..datas.Ki.Value
					textler.Rebirth.Text = "Rebirth: "..datas.Rebirth.Value
					textler.PLlvl.Text = "Base Power Level: "..(datas.Strength.Value*datas.Rebirth.Value/10) + datas.Strength.Value
					script.Parent.Parent.Parent[" PlrName"].Text = n.Name
					
					for _,u in pairs(script.Parent.Parent.Parent.FusePlrList:GetChildren()) do
						u:Remove()
					end
					wait()
					
					local uigridlayout = Instance.new("UIGridLayout")
					uigridlayout.CellSize = UDim2.new(0.92, 0, 0.04, 0)
					uigridlayout.CellPadding = UDim2.new(0, 0, 0, 5)
					uigridlayout.Parent = script.Parent.Parent.Parent.FusePlrList
					
					for _,j in pairs(game.Players:GetChildren())do
						local datas1 = game.ReplicatedStorage.Datas
						local datas = datas1
						local TextButton1 = Instance.new("TextButton")	
						local FuseStats = script.Parent.Parent.Parent["Fuse Stats Show"]
						TextButton1.Parent = script.Parent.Parent.Parent.FusePlrList
						TextButton1.BackgroundColor3 = Color3.new(0.792157, 0.792157, 0)
						TextButton1.Size = UDim2.new(0, 200, 0, 50)
						TextButton1.Font = Enum.Font.SourceSans
						TextButton1.TextColor3 = Color3.new(1, 1, 0)
						TextButton1.TextScaled = true
						TextButton1.TextSize = 14
						TextButton1.TextWrapped = true
						TextButton1.Name = j.Name
						TextButton1.Text = j.Name
						if TextButton1.Text == script.Parent.Parent.Parent[" PlrName"].Text then
							TextButton1:remove()
						end
						TextButton1.MouseButton1Click:connect(function()
							FuseStats.Strength.Text = "Strength: "..datas[script.Parent.Parent.Parent[" PlrName"].Text].Strength.Value + datas[j.Name].Strength.Value
							FuseStats.Speed.Text = "Speed: "..datas[script.Parent.Parent.Parent[" PlrName"].Text].Speed.Value + datas[j.Name].Speed.Value
							FuseStats.Ki.Text = "Ki: "..datas[script.Parent.Parent.Parent[" PlrName"].Text].Ki.Value + datas[j.Name].Ki.Value
							FuseStats.Rebirth.Text = "Rebirth: "..datas[script.Parent.Parent.Parent[" PlrName"].Text].Rebirth.Value + datas[j.Name].Rebirth.Value
							FuseStats.PowerLevel.Text = "Fusion Power Level: "..tostring((datas[script.Parent.Parent.Parent[" PlrName"].Text].Strength.Value + datas[j.Name].Strength.Value)*(datas[script.Parent.Parent.Parent[" PlrName"].Text].Rebirth.Value/10 + datas[j.Name].Rebirth.Value/10)+(datas[script.Parent.Parent.Parent[" PlrName"].Text].Strength.Value + datas[j.Name].Strength.Value))
						end)
					end
				end)
			end
		end)
		
	end
	coroutine.resume(coroutine.create(SCRIPT_DVAZ71_FAKESCRIPT))
	function SCRIPT_UUOU84_FAKESCRIPT() -- List.LocalScript 
		local script = Instance.new('LocalScript')
		script.Parent = List
		for _,c in pairs(game.Players:GetChildren())do
			local datas = game.ReplicatedStorage.Datas[c.Name]
			local textler = script.Parent.Parent["Base Stats"]	
			local TextButton = Instance.new("TextButton")
			TextButton.Parent = script.Parent.PlrList
			TextButton.BackgroundColor3 = Color3.new(0.792157, 0.792157, 0)
			TextButton.Size = UDim2.new(0, 200, 0, 50)
			TextButton.Font = Enum.Font.SourceSans
